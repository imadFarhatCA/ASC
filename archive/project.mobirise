        {
          "_name": "contacts01",
          "_sourceTheme": "conceptm4",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"contact1\" group=\"Contacts\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"4\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"range\" inline title=\"Icons\" name=\"iconsCount\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Items\" name=\"itemsCount\" min=\"1\" max=\"4\" step=\"1\" value=\"1\">\n        <header>Icons</header>\n        <input type=\"color\" title=\"Social Icon\" name=\"iconsColor\" value=\"#000000\" selected>\n        <input type=\"checkbox\" title=\"Link Icon\" name=\"showIcon\" checked>\n        <input type=\"color\" title=\"Link Icon\" name=\"iconColor\" value=\"#8b8c8d\" selected condition=\"showIcon\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/conceptm4/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/conceptm4/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"card col-12 col-md-6 col-lg-6 mb-4\">\n                <div class=\"card-wrapper\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">E-Mail\n                    </h3>\n                    <p class=\"card-text align-left mbr-fonts-style pb-5\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">info@artspecialized.com</p>\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Address\n                    </h3>\n                    <p class=\"card-text align-left mbr-fonts-style pb-5\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">Art Specialized Constructions inc.<br>92-5889 av. Centennial<br>Cte-Saint-Luc, Qubec H4W 1T2, Canada</p>\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Social Media Channels</h3>\n                    <div class=\"icons-wrapper\">\n                        <div class=\"icon-wrapper\" mbr-if=\"iconsCount > 0\">\n                            <a href=\"https://www.facebook.com/artSpecializedConstruction/\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\"></span></a>\n                        </div>\n                        <div class=\"icon-wrapper\" mbr-if=\"iconsCount > 1\">\n                            <a href=\"https://www.instagram.com/artspecialized\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\"></span></a>\n                        </div>\n                        <div class=\"icon-wrapper\" mbr-if=\"iconsCount > 2\">\n                            <a href=\"https://www.linkedin.com/company/artspecializedconstruction\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont socicon-linkedin socicon\"></span></a>\n                        </div>\n                    </div>\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Credentials\n                    </h3>\n                    <div class=\"link-wrapper mt-4\">\n                        <span mbr-icon class=\"mbr-iconfont mobi-mbri-down mobi-mbri\" mbr-if=\"showIcon\"></span>\n                        <div class=\"link-text-box\">\n                            <p class=\"link-text pt-2 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-text\">wether you prefer phone calls, emails, social media channels, we are always available to answer any inquiries</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6 col-md-6\">\n                <div class=\"item\" mbr-if=\"itemsCount > 0\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Phone Number</h3>\n                    <p class=\"card-text align-left mbr-fonts-style pb-5\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">+1 (514) 416 7330</p>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount > 1\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">For Europe Contact</h3>\n                    <p class=\"card-text align-left mbr-fonts-style pb-5\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">1486 Marigold Lane<br>265-81-5641<br></p>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount > 2\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">For China Contact</h3>\n                    <p class=\"card-text align-left mbr-fonts-style pb-5\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\">4946 Kincheloe Road<br>543-78-2005<br></p>\n                </div>\n                <div class=\"item\" mbr-if=\"itemsCount > 3\">\n                    <h3 class=\"card-title align-left mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">For Asia Pacific Contact</h3>\n                    <p class=\"card-text align-left mbr-fonts-style pb-5\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\"></p>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
